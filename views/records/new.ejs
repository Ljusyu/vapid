<h1 class="ui header">New <%- group.displayName %></h1>

<form class="ui form" action="../" method="POST">
  <% for (let fieldName in group.fields) { %>
    <div class="field">
      <label><%- fieldName %></label>
      <%- group.fieldInput(fieldName) %>
    </div>
  <% } %>

  <input class="ui button" type="submit" value="Save">
</form>
